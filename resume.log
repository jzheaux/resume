/*C5*/SET SCHEMA PUBLIC
drop table degree if exists
drop table job if exists
drop table profile if exists
drop table responsibility if exists
drop table resume if exists
drop table skill if exists
create table degree (id bigint not null, AREA_OF_STUDY varchar(255), degree varchar(255), graduation bigint, institution varchar(255), RESUME_ID bigint, primary key (id))
create table job (id bigint not null, company varchar(255), endDate bigint, position varchar(255), startDate bigint, RESUME_ID bigint, primary key (id))
create table profile (id bigint not null, address varchar(255), blog varchar(255), city varchar(255), email varchar(255), FACEBOOK_HANDLE varchar(255), FIRST_NAME varchar(255), GITHUB_HANDLE varchar(255), LAST_NAME varchar(255), LINKEDIN_HANDLE varchar(255), PHONE_NUMBER varchar(255), profession varchar(255), state varchar(255), TWITTER_HANDLE varchar(255), primary key (id))
create table responsibility (id bigint not null, description varchar(400), JOB_ID bigint, primary key (id))
create table resume (id bigint not null, objective varchar(255), PROFILE_ID bigint, primary key (id))
create table skill (id bigint not null, proficiency varchar(255), skill varchar(255), years double, RESUME_ID bigint, primary key (id))
alter table degree add constraint FK_mj3p4lej510uqr500q5f4cd3g foreign key (RESUME_ID) references resume
alter table job add constraint FK_6615jnsmke530w611swo12y2n foreign key (RESUME_ID) references resume
alter table responsibility add constraint FK_cothm5m3d37cjjg50q3nsgkmr foreign key (JOB_ID) references job
alter table resume add constraint FK_9vcuyyrraixk8w522ej0pvca4 foreign key (PROFILE_ID) references profile
alter table skill add constraint FK_ckuiwcvtp36cb5y3g1y5lxab5 foreign key (RESUME_ID) references resume
